load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "memcache_proto",
    srcs = ["memcache_service.proto"],
    visibility = ["//cmd/vendor/github.com/kubernetes-incubator/reference-docs/vendor/google.golang.org/appengine:__subpackages__"],
)

go_proto_library(
    name = "memcache_go_proto",
    importpath = "google.golang.org/appengine/internal/memcache",
    proto = ":memcache_proto",
    visibility = ["//cmd/vendor/github.com/kubernetes-incubator/reference-docs/vendor/google.golang.org/appengine:__subpackages__"],
